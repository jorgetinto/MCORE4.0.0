}

<script src="@Url.Content("~/Scripts/ActiveHelper/Pais.js")" type="text/javascript"></script>

<div class="panel well bs-component">
    <div class="panel-body ">
        <div class="container-fluid">
            <h4>Editar Pais</h4>
            <div class="row">
                <div id="PaisEdit">
                    @Html.Partial("~/Views/Shared/Paises/_Edit.cshtml", ViewBag.Pais as Cobranza.Core.Application.Dto.PanelParametros.PaisDto)
                </div>
                <div class="col-md-12">
                    <hr />
                </div>
                <div class="col-md-12" id="PaisList">
