@{
    Layout = "~/Views/Shared/PanelParametros/_ParametrosLayout.cshtml";
    ViewBag.Title = "Parámetro Rubro";
}

<script src="@Url.Content("~/Scripts/ActiveHelper/Rubro.js")" type="text/javascript"></script>

<div class="panel well bs-component" id="PanelRubros">
    <div class="panel-body">
        <div class="form-group">
            <div class="row">
                <div class="col-md-5 displyBlock"><h4>Mantenedor de Rubros</h4></div>
                <div class="col-md-3">
                    <a href="@Url.Action("Create", "Rubros")" class="btn btn-success btn-man" id="CrearRubro">
                        <span class="glyphicon glyphicon-plus"></span>
                        Nuevo Valor
                    </a>
                </div>
                <div class="col-md-3">
                    <a href="@Url.Action("Index", "PanelParametros")" class="btn btn-danger btn-man" id="Cancelar">
                        <span class="glyphicon glyphicon-remove"></span>
                        Cerrar
                    </a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" id="RubrosList">
            </div>
        </div>
    </div>
</div>